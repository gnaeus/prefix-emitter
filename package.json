{
  "name": "prefix-emitter",
  "version": "0.1.0",
  "description": "Simple Event Emitter for ES6 and TypeScript based on Prefix Tree",
  "main": "dist/prefix-emitter.js",
  "typings": "dist/prefix-emitter.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "webpack & build-typings",
    "build-dev": "webpack --watch",
    "build-typings": "tsc -p tsconfig.typings.json && ncp dist/__typings/prefix-emitter.d.ts dist/prefix-emitter.d.ts && rimraf dist/__typings",
    "prepublish": "typings install",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gnaeus/prefix-emitter.git"
  },
  "keywords": [
    "event",
    "emitter",
    "pub/sub",
    "trie"
  ],
  "author": "Dmitry Panyushkin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gnaeus/prefix-emitter/issues"
  },
  "homepage": "https://github.com/gnaeus/prefix-emitter#readme",
  "devDependencies": {
    "deepmerge": "^0.2.10",
    "jest-cli": "^14.0.1",
    "ncp": "^2.0.0",
    "rimraf": "^2.5.4",
    "ts-loader": "^0.8.2",
    "typescript": "^1.8.10",
    "typings": "^1.3.2",
    "webpack": "^1.13.1"
  },
  "jest": {
    "rootDir": "./src",
    "scriptPreprocessor": "../jest-preprocessor.js",
    "testFileExtensions": [
      "js",
      "ts"
    ],
    "moduleFileExtensions": [
      "js",
      "ts"
    ]
  }
}
