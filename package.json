    "emitter",
    "pub/sub",
    "trie"
  ],
  "author": "Dmitry Panyushkin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gnaeus/prefix-emitter/issues"
  },
  "homepage": "https://github.com/gnaeus/prefix-emitter#readme",
  "devDependencies": {
    "deepmerge": "^0.2.10",
    "dts-generator": "^1.7.0",
    "jest-cli": "^14.0.1",
    "ts-loader": "^0.8.2",
    "typescript": "^1.8.10",
    "typings": "^1.3.2",
    "webpack": "^1.13.1"
  },
  "jest": {
    "rootDir": "./src",
    "scriptPreprocessor": "../jest-preprocessor.js",
